{
	"profiles": {
		"Terraria": {
			"commandName": "Executable",
			"executablePath": "dotnet",
			"commandLineArgs": "\"$(tMLPath)\" -skipselect \"Mod Testing:Mod Testing\"",
			"workingDirectory": "$(tMLSteamPath)",
			"environmentVariables": {
				"TXM_MODSOURCEPATH": "$(ProjectDir)"
			}
		},
		"TerrariaServer": {
			"commandName": "Executable",
			"executablePath": "dotnet",
			"commandLineArgs": "$(tMLServerPath)",
			"workingDirectory": "$(tMLSteamPath)"
		},
		"Terraria (hot reload+ watcher)": {
			"commandName": "Project",
			"commandLineArgs": "\"$(TerrariaSteamPath)$(tMLPath)\" -skipselect \"Mod Testing:Mod Testing\""
		},
	}
}